<?xml version="1.0" encoding="utf-8" ?>
<widget id="DoubleSlider.widget.DoubleSlider" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>DoubleSlider</name>
    <description>A Range Slider input control for Mendix.</description>
	
	<!-- Icon made by Vaadin www.flaticon.com --> 
    <icon></icon>

    <properties>
        <property key="fromValueAttr" type="attribute">
            <caption>Left value</caption>
            <category>Data Source</category>
            <description>The left value on the range.</description>
            <attributeTypes>
                <attributeType name="Integer" />
                <attributeType name="Long" />
                <attributeType name="Float" />
            </attributeTypes>
        </property>        
		<property key="toValueAttr" type="attribute" required="false">
            <caption>Right value</caption>
            <category>Data Source</category>
            <description>The right value on the range, can be empty.</description>
            <attributeTypes>
                <attributeType name="Integer" />
                <attributeType name="Long" />
                <attributeType name="Float" />
            </attributeTypes>
        </property>
        <property key="minAttr" type="attribute">
            <caption>Min Attribute</caption>
            <category>Data Source</category>
            <description>The min for the range.</description>
            <attributeTypes>
                <attributeType name="Integer" />
                <attributeType name="Long" />
                <attributeType name="Float" />
            </attributeTypes>
        </property>
        <property key="maxAttr" type="attribute">
            <caption>Max Attribute</caption>
            <category>Data Source</category>
            <description>The max for the range.</description>
            <attributeTypes>
                <attributeType name="Integer" />
                <attributeType name="Long" />
                <attributeType name="Float" />
            </attributeTypes>
        </property>
        <property key="stepAttr" type="attribute" required="false">
            <caption>Range Step Attribute</caption>
            <category>Data Source</category>
            <description>The step value of the range from an attribute, to 5% of the range.</description>
            <attributeTypes>
                <attributeType name="Integer" />
                <attributeType name="Long" />
                <attributeType name="Float" />
            </attributeTypes>
        </property>
        <property key="orientation" type="enumeration" defaultValue="horizontal">
            <caption>Orientation</caption>
            <category>Appearance</category>
            <description>Slider orientation: horizontal (recommended) or vertical</description>
            <enumerationValues>
                <enumerationValue key="horizontal">Horizontal</enumerationValue>
                <enumerationValue key="vertical">Vertical</enumerationValue>
            </enumerationValues>
        </property>
    </properties>
</widget>

